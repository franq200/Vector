        <!-- Tekstowa reprezentacja obiektu w oknie debugera -->
        <DisplayString>Vector of {m_size} elements (Capacity: {m_capacity})</DisplayString>
        
        <!-- Rozwini�cie obiektu pokazuj�ce kluczowe pola -->
